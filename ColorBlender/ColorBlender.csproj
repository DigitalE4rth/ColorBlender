<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Description>Mixing colors using different blending modes with C#</Description>
    <PackageProjectUrl>https://github.com/DigitalE4rth/ColorBlender</PackageProjectUrl>
    <PackageReadmeFile>Readme.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/DigitalE4rth/ColorBlender</RepositoryUrl>
    <Title>Color Blender</Title>
    <Authors>DigitalE4rth</Authors>
    <Copyright>https://github.com/DigitalE4rth/ColorBlender</Copyright>
    <PackageTags>Color, Converter, Blend, Blending, Blender, RGB, HSL, HEX</PackageTags>
    <PackageIcon>Icon.png</PackageIcon>
    <PackageId>DigitalE4rth.ColorBlender</PackageId>
    <Version>1.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\Art\Icon.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\Readme.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
